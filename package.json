{
  "name": "autocomplete-sofistik",
  "description": "Get hints as you type the SOFiSTiK input",
  "repository": "https://github.com/asiloisad/pulsar-autocomplete-sofistik",
  "keywords": [
    "autocomplete",
    "sofistik"
  ],
  "license": "MIT",
  "version": "2.0.0",
  "engines": {
    "atom": ">=1.100.0 <2.0.0"
  },
  "main": "./lib/main",
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "4.0.0": "getProvider"
      }
    }
  },
  "consumedServices": {
    "sofistik.keywords": {
      "versions": {
        "^1.0.0": "consumeKeywordsService"
      }
    }
  },
  "configSchema": {
    "textCase": {
      "order": 1,
      "title": "Suggestions case",
      "description": "Convert case for suggestion elements.",
      "type": "boolean",
      "enum": [
        {
          "value": true,
          "description": "Uppercase"
        },
        {
          "value": false,
          "description": "Lowercase"
        }
      ],
      "default": true
    }
  }
}
